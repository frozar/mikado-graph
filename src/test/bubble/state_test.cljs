(ns bubble.state-test
  (:require [bubble.state :as state]
            [cljs.test :refer (deftest is)]
            [cljs.pprint :refer (pprint)]
            ))

(deftest failing
  (pprint (state/init-appstate))
  (is
   (=
    1
    1)))

